        <activity android:name=".Manager_student_list"
            android:parentActivityName=".Main2Activity"
            android:exported="true"/>
        <activity android:name=".Manager_today_menu"
            android:parentActivityName=".Main2Activity"/>
        <activity android:name=".Manager_new_registration"
            android:parentActivityName=".Main2Activity"
            android:exported="true"/>
        <activity android:name=".Manager_complaints"
            android:parentActivityName=".Main2Activity"
            android:exported="true"/>
        <activity android:name=".Manager_create_polling"
            android:parentActivityName=".Main2Activity"
